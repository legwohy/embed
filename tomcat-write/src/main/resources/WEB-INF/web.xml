<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<!-- 注册两个服务处理  -->
	<servlet>
		<url-pattern>/login.action</url-pattern>
		<servlet-class>com.myTomcat.servlet.LoginServlet</servlet-class>
	</servlet>

	<servlet>
		<url-pattern>/register.action</url-pattern>
		<servlet-class>com.myTomcat.servlet.RegisterServlet</servlet-class>
	</servlet>
	
</web-app>